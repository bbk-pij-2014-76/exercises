println (" Please enter a number ");
String s = readLine();
carthy91 num = new carthy91();
num.n = Integer.parseInt(s);
println (num.mcCarthy91 (num.n));

class carthy91 {
	int n;
	
	int mcCarthy91(int n) {
		if (n > 100) {
			return n - 10;
		} else {
			return mcCarthy91(mcCarthy91(n+11));
		}
	}
}
